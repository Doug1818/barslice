<ul class="rooms">
	<% object.specials.each do |special| %>
	<li class="room">
		<div>
			<strong>Name:</strong>
			<%= special.name %>
		</div>
			<%= render 'bars/special', special: special %>
		</li>
	<% end %>
</ul>
<% if bar_signed_in? %>
<%= link_to "Add special", new_special_path, class: "btn btn-med btn-primary" %>
<% end %>